{
    "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
    },
    "extends": [
      "standard"
    ],
    "parserOptions": {
      "ecmaVersion": 11
    },
    "rules": {
      "curly": "off",
      "no-extend-native": "off",
      "func-call-spacing": "off"
    },
    "globals": {
      "sap": "readonly"
    }
}
